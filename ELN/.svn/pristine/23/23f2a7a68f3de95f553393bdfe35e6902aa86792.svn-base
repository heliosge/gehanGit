package com.jftt.wifi.bean.thematicAsk;

import java.util.Date;

/**
 * class name:ManageThematicAskVo <BR>
 * class description: 专题问答管理vo <BR>
 * Remark: <BR>
 * @version 1.00 2015年11月25日
 * @author JFTT)ShenHaijun
 */
public class ManageThematicAskVo {
	//展现内容
	private Integer id;//专题问答id
	private String title;//标题
	private String contentSummary;//内容摘要
	private Date askTime;//提问时间
	private Date effectiveTime;//问题截止有效期
	private Integer replyCount;//回复数量
	private Integer askState;//问题状态（1：未处理 2：已处理 3：已过期 0：其他状态）
	private Integer askerId;//提问人id
	//查询条件
	private String searchAskState;//问题状态（1：未处理 2：已处理 3：已过期）
	private String searchTitle;//标题
	//分页排序
	private Integer fromNum;//起始条数
	private Integer pageSize;//每页大小
	
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	public String getContentSummary() {
		return contentSummary;
	}
	public void setContentSummary(String contentSummary) {
		this.contentSummary = contentSummary;
	}
	public Date getAskTime() {
		return askTime;
	}
	public void setAskTime(Date askTime) {
		this.askTime = askTime;
	}
	public Date getEffectiveTime() {
		return effectiveTime;
	}
	public void setEffectiveTime(Date effectiveTime) {
		this.effectiveTime = effectiveTime;
	}
	public Integer getReplyCount() {
		return replyCount;
	}
	public void setReplyCount(Integer replyCount) {
		this.replyCount = replyCount;
	}
	public Integer getAskState() {
		return askState;
	}
	public void setAskState(Integer askState) {
		this.askState = askState;
	}
	public Integer getAskerId() {
		return askerId;
	}
	public void setAskerId(Integer askerId) {
		this.askerId = askerId;
	}
	public String getSearchAskState() {
		return searchAskState;
	}
	public void setSearchAskState(String searchAskState) {
		this.searchAskState = searchAskState;
	}
	public String getSearchTitle() {
		return searchTitle;
	}
	public void setSearchTitle(String searchTitle) {
		this.searchTitle = searchTitle;
	}
	public Integer getFromNum() {
		return fromNum;
	}
	public void setFromNum(Integer fromNum) {
		this.fromNum = fromNum;
	}
	public Integer getPageSize() {
		return pageSize;
	}
	public void setPageSize(Integer pageSize) {
		this.pageSize = pageSize;
	}
}
