package com.jftt.wifi.bean;

import java.util.Date;

import com.jftt.wifi.bean.exam.ExamScheduleBean;

/**
 * @author JFTT)wangyifeng
 * class name:KnowledgeContestMatchBean <BR>
 * class description: 大赛赛程Bean <BR>
 * Remark: <BR>
 * @version 1.00 2015/08/13
 */
public class KnowledgeContestMatchBean {
    /**  
     * 主键ID
     */
    private Integer id;

    /**  
     * 大赛ID
     */
    private Integer contestId;

    /**  
     * 赛程序号
     */
    private Integer orderNum;

    /**  
     * 赛程名称
     */
    private String name;

    /**  
     * 赛程内容
     */
    private String content;

    /**  
     * 考试ID
     */
    private Integer examId;

    /**  
     * 考试Bean
     */
    private ExamScheduleBean exam;

    /**  
     * 评审规则
     */
    private String rule;

    /**  
     * 晋级人数
     */
    private Integer promotionNumber;
    private Integer subCompanyId;
    private Integer companyId;
    /**  
     * 更新时间
     */
    private Date updateTime;
    /**  
     * 赛程状态
     */
    private Integer status;

    public Integer getId() {
        return id;
    }

    public void setId(Integer id) {
        this.id = id;
    }

    public Integer getContestId() {
        return contestId;
    }

    public void setContestId(Integer contestId) {
        this.contestId = contestId;
    }

    public Integer getOrderNum() {
        return orderNum;
    }

    public void setOrderNum(Integer orderNum) {
        this.orderNum = orderNum;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getContent() {
        return content;
    }

    public void setContent(String content) {
        this.content = content;
    }

    public Integer getExamId() {
        return examId;
    }

    public void setExamId(Integer examId) {
        this.examId = examId;
    }

    public String getRule() {
        return rule;
    }

    public void setRule(String rule) {
        this.rule = rule;
    }

    public Integer getPromotionNumber() {
        return promotionNumber;
    }

    public void setPromotionNumber(Integer promotionNumber) {
        this.promotionNumber = promotionNumber;
    }

    public Date getUpdateTime() {
        return updateTime;
    }

    public void setUpdateTime(Date updateTime) {
        this.updateTime = updateTime;
    }

	public ExamScheduleBean getExam() {
		return exam;
	}

	public void setExam(ExamScheduleBean exam) {
		this.exam = exam;
	}

	public Integer getStatus() {
		return status;
	}

	public void setStatus(Integer status) {
		this.status = status;
	}

	public Integer getSubCompanyId() {
		return subCompanyId;
	}

	public void setSubCompanyId(Integer subCompanyId) {
		this.subCompanyId = subCompanyId;
	}

	public Integer getCompanyId() {
		return companyId;
	}

	public void setCompanyId(Integer companyId) {
		this.companyId = companyId;
	}
	
}
