package com.jftt.wifi.bean;

public class ResCourseOpenCrowdBean {
    private Integer id;

    private Integer courseId;

    private Integer openType;//1：全部人员；2：岗位；3：群组；4：部门

    private Integer openCrowId;

    public Integer getId() {
        return id;
    }

    public void setId(Integer id) {
        this.id = id;
    }

    public Integer getCourseId() {
        return courseId;
    }

    public void setCourseId(Integer courseId) {
        this.courseId = courseId;
    }

    public Integer getOpenType() {
        return openType;
    }

    public void setOpenType(Integer openType) {
        this.openType = openType;
    }

    public Integer getOpenCrowId() {
        return openCrowId;
    }

    public void setOpenCrowId(Integer openCrowId) {
        this.openCrowId = openCrowId;
    }
}