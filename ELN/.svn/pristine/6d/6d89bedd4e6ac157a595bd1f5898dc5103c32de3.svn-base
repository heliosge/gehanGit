package com.jftt.wifi.bean.exam;

import java.util.ArrayList;
import java.util.List;

/**
 * class QuestionCategoryBean <BR>
 * class description: 试题分类Bean <BR>
 * Remark: <BR>
 * 
 * @version 1.00 2015/07/22
 * @author JFTT)wangyifeng
 */
public class QuestionCategoryBean extends CategoryBaseBean {
	/**  
	 * 子试题分类
	 */
	private List<QuestionCategoryBean> subCategoryList = new ArrayList<QuestionCategoryBean>();
	
	/**  
	 * 试题库中的试题数量,格式为总数,完成数,错误数
	 */
	private String count;

	public List<QuestionCategoryBean> getSubCategoryList() {
		return subCategoryList;
	}

	public void setSubCategoryList(List<QuestionCategoryBean> subCategoryList) {
		this.subCategoryList = subCategoryList;
	}

	public String getCount() {
		return count;
	}

	public void setCount(String count) {
		this.count = count;
	}
	
}
