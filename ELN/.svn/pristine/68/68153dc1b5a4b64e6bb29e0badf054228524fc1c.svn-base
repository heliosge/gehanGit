package com.jftt.wifi.bean.ask;

/**
 * class name:EditAskTypeVo <BR>
 * class description: 问问分类编辑框内容 <BR>
 * Remark: <BR>
 * @version 1.00 2015年11月19日
 * @author JFTT)ShenHaijun
 */
public class EditAskTypeVo {
	private Integer typeId;//分类id
	private String typeName;//分类名称
	private String description;//分类描述
	private Integer parentTypeId;//父级分类id
	private String parentTypeName;//父级分类名称
	
	public Integer getTypeId() {
		return typeId;
	}
	public void setTypeId(Integer typeId) {
		this.typeId = typeId;
	}
	
	public String getTypeName() {
		return typeName;
	}
	public void setTypeName(String typeName) {
		this.typeName = typeName;
	}
	public String getDescription() {
		return description;
	}
	public void setDescription(String description) {
		this.description = description;
	}
	public Integer getParentTypeId() {
		return parentTypeId;
	}
	public void setParentTypeId(Integer parentTypeId) {
		this.parentTypeId = parentTypeId;
	}
	public String getParentTypeName() {
		return parentTypeName;
	}
	public void setParentTypeName(String parentTypeName) {
		this.parentTypeName = parentTypeName;
	}
}
