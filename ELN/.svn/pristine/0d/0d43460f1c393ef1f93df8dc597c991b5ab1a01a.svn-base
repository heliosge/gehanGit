@charset "utf-8";
/* CSS Document */

/*课程商城*/
.content_mall{ width:1046px; margin:0 auto;}
.head_mall{ width:1046px; padding:20px 0 20px 20px;float:left;}
.head_mall h4{ float:left; color:#8b8b8b; margin-right:10px;}
.head_mall span{ margin-right:10px; color:#8b8b8b;}

.nav_mall{ width:220px; float:left;}
.nav_mall h3{ width:206px; background:#188eee; height:50px; line-height:50px; color:#fff; font-weight:normal; padding-left:14px; font-size:16px;}
.nav_mall img{ margin-top:16px; margin-right:6px;}
.nav_mall ul li{ width:188px; padding-left:14px; height:50px; line-height:50px; background:#0477c0; padding-right:18px; border-bottom:1px solid #4298cf;}
.nav_mall ul li span{ color:#4b9ed2;}
.nav_mall ul .li_f{ width:188px; padding-left:14px; height:50px; line-height:50px; color:#fff; background:#0264a3; padding-right:18px; border-bottom:1px solid #4298cf;}
.nav_mall ul .li_f a{ font-weight:bold; font-size:14px;}
.nav_mall ul .li_f span{ color:#fff;}
.nav_mall ul li a{ color:#fff; font-size:14px;}
.nav_mall ul li span{ float:right;}

.mall_list{ width:796px; margin-left:30px; float:left; position:relative;}
.list_top{ width:786px; padding-bottom:20px;}
.input_m{ width:308px; height:46px; border:1px solid #cccccc; text-indent:1em; color:#d0d0d0; font-size:14px;}
.button_1{ width:200px; height:48px; border:none; background:#feda4e; color:#404143; font-weight:bold; line-height:48px; text-align:center;}
.img_4{ position:absolute; left:270px; top:10px;}
.list_top .shop{ margin-left:40px;}
.list_top .px{ float:right; height:40px; line-height:40px;padding-top:10px;}
.list_top .px a{ color:#424146; float:left; width:80px; border-right:1px solid #c4c4c4; height:30px; line-height:30px; text-align: center; background:url(../img/ico_dw1.png) no-repeat 60px center;}
.list_top .px a:hover{ color:#F00;}
.list_top .px .a_last{ border:none;}

.list_tb{ width:796px;}
.list_tb ul li{ float:left; width:240px; height:226px; border:1px solid #cccccc; margin-right:34px; margin-bottom:36px;}
.list_tb ul li:hover{ box-shadow:2px 4px 4px 2px #ccc;}
.list_tb ul .last_li_1{ margin-right:0;}
.list_tb ul li .tb_top{ height:151px;}
.list_tb ul li p{ padding:12px 20px 14px 10px; font-family:'微软雅黑';}
.list_tb ul li p span{ float:left; color:#33342f;}
.list_tb ul li p strong{ float:right; color:#ea5407; font-weight:normal;}
.list_tb ul li p .c_pj{ color:#9a9899;}
.list_tb ul li p b{color:#ff7335; font-size:14px; float:left; font-weight:normal;}
.list_tb ul li p a{ float:left; margin-left:10px; margin-right:10px;}
.list_tb ul li p em{ color:#fb752e;}

.gw_top{ width:100%;min-width:1046px; height:60px; border-bottom:1px solid #ccc;}
.gw_top h4{ width:1046px; margin:0 auto; height:30px; padding-top:30px; background:url(../img/ico_shop.png) no-repeat left center; text-indent:4em; color:#8b8b8b; font-size:14px;}
.gw_list{ width:1046px; margin:0 auto;}
.gw_list .list_sc{ width:1046px; height:72px; padding-top:30px; position:relative;}
.gw_list .list_sc input[type=text]{ width:312px; height:42px; border:1px solid #ccc; color:#d0d0d0; text-indent:1em;}
.gw_list .list_sc a{ position:absolute; left:270px; top:36px;}

.list_sp{ width:1046px;}
.list_sp .sp_1{ width:1036px; height:46px; border-top:1px solid #efefef; padding-left:10px;}
.list_sp .sp_1 li{ height:46px; line-height:46px; width:60px; margin-right:170px; float:left;}
.list_sp .sp_1 li input[type=checkbox]{ vertical-align:middle;}
.list_sp .sp_1 .last_3{ margin-right:0;}
.dp{ width:1026px;  height:22px; padding-top:38px; border-bottom:1px solid #efefef; border-top:1px solid #efefef; margin:0 auto;}
.dp span{ color:#9a9a9a;}
.dp input[type=checkbox]{ vertical-align:middle;}
.dp b{ font-weight:normal; color:#232323;}
.list_sp .sp_2{ width:1026px; height:112px;padding-left:10px;}
.list_sp .sp_2 li{ height:112px; line-height:112px; width:60px; margin-right:170px; float:left;}
.list_sp .sp_2 li span{ color:#9e9e9e;}
.list_sp .sp_2 li span a{ color:#9e9e9e;}
.list_sp .sp_2 li span a:hover{color:#ff4e7d; text-decoration:underline;}
.list_sp .sp_2 li strong{ color:#ff4e7d; font-family:'微软雅黑'; font-size:14px;}
.list_sp .sp_2 .li_fir{ margin-right:30px;}
.list_sp .sp_2 .li_2{ width:200px; margin-left:24px; margin-right:146px;}
.list_sp .sp_2 li img{ vertical-align:middle;}
.list_sp .sp_2 li input[type=checkbox]{ vertical-align:middle;}
.list_sp .sp_2 .last_3{ margin-right:0;}
.list_sp .dp_1{ width:1036px; padding-left:10px; height:50px; border-top:1px solid #ccc; line-height:50px; border-bottom:1px solid #efefef; }
.list_sp .dp_1 input[type=checkbox]{ vertical-align:middle;}
.list_sp .dp_1 span{ margin-left:4px;}
.list_sp .dp_1 a{ color:#666666; text-decoration:underline;}
.list_sp .dp_1 a:hover{ color:#ff4e7d;}
.list_sp .dp_1 strong{ float:right; font-weight:normal; margin-right:12px;}
.list_sp .dp_1 strong b{ font-weight:normal; color:#ff4fa4;}
.list_sp .dp_1 strong em{ color:#fe4e7c; font-size:20px; font-weight:bold; font-family:'微软雅黑';}
.list_sp .dp_1 input[type=button]{ float:right; background:#d8d8d8; height:50px; border:none; text-align:center; width:140px; color:#fff; font-size:20px; font-family:'微软雅黑';}

.pay{ width:920px; padding-top:20px; padding-left:20px; padding-right:20px; margin:0 auto;}
.pay h4{ font-size:14px; margin-bottom:20px;}
.pay h4 span{ font-weight:normal; color:#666;}
.pay p{ width:960px; margin-bottom:20px; font-size:14px;}
.pay p em{ margin-right:20px;}
.pay p span{ color:#F00; margin-right:4px;}
.pay .tabl_1{ margin-bottom:20px;}
.pay .tabl_1 th{ width:120px; background:#f2f2f2; height:38px; border:1px solid #ccc; font-weight:normal; text-align:right; padding-right:20px;}
.pay .tabl_1 td{ width:270px;height:38px; border:1px solid #ccc; font-weight:normal; text-align:left; padding-left:20px;}

.pay .tabl_2{ margin-bottom:20px;}
.pay .tabl_2 th{ width:130px; background:#f2f2f2; height:38px; border:1px solid #ccc; font-weight:normal; text-align:right; padding-right:20px;}
.pay .tabl_2 th span{ color:#F00;}
.pay .tabl_2 td{height:38px; border:1px solid #ccc; font-weight:normal; text-align:left; padding-left:20px;}
.pay .tabl_2 td input[type=text]{ width:316px; height:24px; border:1px solid #ccc;}
.pay .tabl_2 td select{ width:316px; height:24px; border:1px solid #ccc;}

.s_sp{ width:680px;}
.s_sp .tabl_1{ margin-bottom:20px;}
.s_sp .tabl_1 th{ width:30%; background:#f2f2f2; height:38px; border:1px solid #ccc; font-weight:normal; text-align:right; padding-right:20px;}
.s_sp .tabl_1 td{height:38px; border:1px solid #ccc; font-weight:normal; text-align:left; padding-left:20px;}
.s_sp .tabl_1 td img{ margin-right:6px;}
.s_sp .tabl_1 td p{ line-height:24px;}
