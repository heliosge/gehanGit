package com.jftt.wifi.bean.vo;

import java.util.Date;

/**
 * class name:CourseNoteVo <BR>
 * class description: 课程笔记vo（课程学习页面） <BR>
 * Remark: <BR>
 * @version 1.00 2015年8月28日
 * @author JFTT)ShenHaijun
 */
public class CourseNoteVoForStudy {
	private Integer id;//笔记id
	private Integer userId;//笔记记录人id
	private String userName;//笔记记录人姓名
	private String content;//笔记内容
	private Date createTime;//笔记创建时间
	
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public Integer getUserId() {
		return userId;
	}
	public void setUserId(Integer userId) {
		this.userId = userId;
	}
	public String getUserName() {
		return userName;
	}
	public void setUserName(String userName) {
		this.userName = userName;
	}
	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}
	public Date getCreateTime() {
		return createTime;
	}
	public void setCreateTime(Date createTime) {
		this.createTime = createTime;
	}
}
