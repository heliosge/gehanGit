package com.jftt.wifi.bean.vo;

import java.math.BigDecimal;

import com.itextpdf.text.List;
import com.jftt.wifi.bean.ResSectionBean;

/**
 * class name:MallCoursePageVo <BR>
 * class description: <BR>
 * Remark: <BR>
 * @version 1.00 2015年11月17日
 * @author JFTT)chenrui
 */
public class MallCoursePageVo {
	private int id;
	private String name;
	private String code;//课程代码
	private String descr;//课程描述
	private BigDecimal price;//原价
	private BigDecimal cheapPrice;//优惠价
	private int avgScore;//平均评分
	private int evaluateNum;//评价人数
	private String img;//课程封面图片
	private int courseId;//课程id
	private String categroyName;
	private int companyId;//课程所属企业Id
	private int subCompanyId;
	private String companyName;//课程所属企业名称
	private String productType;//商品类型 1课程 2专题
	private String isEffective;//1有效 2失效  （购物车中的商品标识是否时效  ）
	
	private int productId;//购物车查数据时用  购物车中的商品id  此时当前bean中id为购物车主键id
	
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	
	public String getCode() {
		return code;
	}
	public void setCode(String code) {
		this.code = code;
	}
	public BigDecimal getPrice() {
		return price;
	}
	public void setPrice(BigDecimal price) {
		this.price = price;
	}
	public BigDecimal getCheapPrice() {
		return cheapPrice;
	}
	public void setCheapPrice(BigDecimal cheapPrice) {
		this.cheapPrice = cheapPrice;
	}
	public int getAvgScore() {
		return avgScore;
	}
	public void setAvgScore(int avgScore) {
		this.avgScore = avgScore;
	}
	public int getEvaluateNum() {
		return evaluateNum;
	}
	public void setEvaluateNum(int evaluateNum) {
		this.evaluateNum = evaluateNum;
	}
	public String getImg() {
		return img;
	}
	public void setImg(String img) {
		this.img = img;
	}
	public int getCourseId() {
		return courseId;
	}
	public void setCourseId(int courseId) {
		this.courseId = courseId;
	}
	public String getCategroyName() {
		return categroyName;
	}
	public void setCategroyName(String categroyName) {
		this.categroyName = categroyName;
	}
	public String getProductType() {
		return productType;
	}
	public void setProductType(String productType) {
		this.productType = productType;
	}
	public String getIsEffective() {
		return isEffective;
	}
	public void setIsEffective(String isEffective) {
		this.isEffective = isEffective;
	}
	public String getDescr() {
		return descr;
	}
	public void setDescr(String descr) {
		this.descr = descr;
	}
	public int getCompanyId() {
		return companyId;
	}
	public void setCompanyId(int companyId) {
		this.companyId = companyId;
	}
	
	public int getSubCompanyId() {
		return subCompanyId;
	}
	public void setSubCompanyId(int subCompanyId) {
		this.subCompanyId = subCompanyId;
	}
	public String getCompanyName() {
		return companyName;
	}
	public void setCompanyName(String companyName) {
		this.companyName = companyName;
	}
	public int getProductId() {
		return productId;
	}
	public void setProductId(int productId) {
		this.productId = productId;
	}
	
	
}
