public class ExerciseQuestionBean {
	/**  
	 * 主键ID
	 */
	private Integer id;
	/**  
	 * 用户ID
	 */
	private Integer userId;
	/**  
	 * 二级题库ID
	 */
	private Integer categoryId;
	/**  
	 * 一级题库ID
	 */
	private Integer parentCategoryId;
	/**  
	 * 试题ID
	 */
	private Integer questionId;
	
	/**  
	 * 错误标记0-对，1-错
	 */
	private Integer isWrong;
	
	/**  
	 * 试题库IDSTr
	 */
	private String categoryIdListStr;
	
	
	
	public String getCategoryIdListStr() {
		return categoryIdListStr;
	}
	public void setCategoryIdListStr(String categoryIdListStr) {
		this.categoryIdListStr = categoryIdListStr;
	}
	public Integer getParentCategoryId() {
		return parentCategoryId;
	}
	public void setParentCategoryId(Integer parentCategoryId) {
		this.parentCategoryId = parentCategoryId;
	}
	public Integer getIsWrong() {
		return isWrong;
	}
	public void setIsWrong(Integer isWrong) {
		this.isWrong = isWrong;
	}
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public Integer getUserId() {
		return userId;
	}
	public void setUserId(Integer userId) {
		this.userId = userId;
	}
	
	public Integer getCategoryId() {
		return categoryId;
	}
	public void setCategoryId(Integer categoryId) {
		this.categoryId = categoryId;
	}
	public Integer getQuestionId() {
		return questionId;
	}
	public void setQuestionId(Integer questionId) {
		this.questionId = questionId;
	}
	
	

}
