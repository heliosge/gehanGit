package com.jftt.wifi.bean.ask;

/**
 * class name:RootAskTypeVo <BR>
 * class description: 问问页面根分类vo <BR>
 * Remark: <BR>
 * @version 1.00 2015年12月9日
 * @author JFTT)ShenHaijun
 */
public class RootAskTypeVo {
	private Integer id;//分类id
	private String name;//分类名称
	private Integer askCount;//该分类下问题数量
	
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public Integer getAskCount() {
		return askCount;
	}
	public void setAskCount(Integer askCount) {
		this.askCount = askCount;
	}
}
